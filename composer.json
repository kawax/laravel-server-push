{
  "name": "revolution/laravel-server-push",
  "description": "Laravel Server Push Middleware",
  "keywords": [
    "server-push",
    "http2",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1.3",
    "ext-json": "*",
    "illuminate/support": "^5.5",
    "illuminate/http": "^5.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5",
    "mockery/mockery": "^1.0",
    "orchestra/testbench": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "Revolution\\ServerPush\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "kawax",
      "email": "kawaxbiz@gmail.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Revolution\\ServerPush\\Providers\\ServerPushServiceProvider"
      ]
    }
  }
}
